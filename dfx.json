{
  "version": 1,
  "dfx": "0.8.4",
  "canisters": {
    "icfs": {
      "type": "custom",
      "build": "nix build '.#icfs-example'",
      "candid": "examples/icfs/icfs.did",
      "wasm": "result/lib/icfs_example.wasm"
    },
    "ext4": {
      "type": "custom",
      "build": "nix build '.#ext4-example'",
      "candid": "examples/ext4/ext4.did",
      "wasm": "result/lib/ext4_example.wasm"
    },
    "fatfs": {
      "type": "custom",
      "build": "nix build '.#fatfs-example'",
      "candid": "examples/fatfs/fatfs.did",
      "wasm": "result/lib/fatfs_example.wasm"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
